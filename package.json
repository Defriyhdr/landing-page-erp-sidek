{
  "name": "erp-fe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && pnpm install",
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview --host",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:graph": "npx depcruise src --include-only '^src' --config --output-type dot | dot -T svg > docs/assets/dependency-graph.svg",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "@types/lodash": "^4.14.194",
    "@types/pica": "^9.0.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.26",
    "@vueuse/integrations": "^9.13.0",
    "@vueuse/router": "^9.13.0",
    "aos": "^2.3.4",
    "apexcharts": "^3.40.0",
    "axios": "^1.4.0",
    "change-case": "^4.1.2",
    "file-saver": "^2.0.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "maska": "^2.1.9",
    "pica": "^9.0.1",
    "pinia": "^2.1.3",
    "postcss": "^8.4.23",
    "v-calendar": "^3.0.3",
    "vee-validate": "^4.9.5",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1",
    "vue-tabler-icons": "^2.20.0",
    "vue-toastification": "2.0.0-rc.5",
    "vue3-apexcharts": "^1.4.1",
    "vue3-carousel": "^0.2.16",
    "vue3-easy-data-table": "^1.5.42",
    "vue3-lottie": "^2.7.0",
    "vuedraggable": "^2.24.3",
    "vuetify": "^3.3.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@mdi/js": "^7.2.96",
    "@rushstack/eslint-patch": "^1.3.0",
    "@types/aos": "^3.0.4",
    "@types/chance": "^1.1.3",
    "@types/file-saver": "^2.0.5",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.14",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "chance": "^1.1.11",
    "dependency-cruiser": "^12.12.2",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.0",
    "jsdom": "^21.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.0",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
